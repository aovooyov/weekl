{
  "name": "weekl",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --aot",
    "build:prod": "ng build --prod --aot --output-hashing=all",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "start:ssr": "npm run build:ssr && node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod --aot --output-hashing=all && ng run weekl:server:production --output-hashing=all",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.9",
    "@angular/common": "^6.0.9",
    "@angular/compiler": "^6.0.9",
    "@angular/core": "^6.0.9",
    "@angular/forms": "^6.0.9",
    "@angular/http": "^6.0.9",
    "@angular/platform-browser": "^6.0.9",
    "@angular/platform-browser-dynamic": "^6.0.9",
    "@angular/router": "^6.0.9",
    "@angular/upgrade": "^6.0.9",
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@ngx-config/core": "^6.0.0",
    "@ngx-config/http-loader": "^6.0.0",
    "@ngx-meta/core": "^6.0.0",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.7",
    "moment": "^2.22.2",
    "ngx-infinite-scroll": "^6.0.1",
    "npm": "^6.4.1",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "^6.0.9",
    "@angular/language-service": "^6.0.9",
    "@angular/platform-server": "^6.0.9",
    "@angular-devkit/build-angular": "~0.6.8",
    "@types/node": "^10.5.2",
    "ts-node": "^7.0.0",
    "ts-loader": "^4.5.0",
    "typescript": "~2.7.2",
    "webpack-cli": "^2.0.14"
  }
}
